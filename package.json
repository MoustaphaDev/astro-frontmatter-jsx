{
    "name": "root",
    "private": true,
    "type": "module",
    "scripts": {
        "start": "pnpm --filter demo run start",
        "build": "turbo run build --output-logs=new-only --no-deps --filter=astro-frontmatter-jsx --concurrency=1",
        "dev": "turbo run dev --output-logs=new-only --no-deps --filter=astro-frontmatter-jsx --concurrency=1",
        "version": "changeset version && pnpm install --no-frozen-lockfile"
    },
    "workspaces": [
        "packages/*",
        "demo",
        "scripts"
    ],
    "devDependencies": {
        "@changesets/changelog-github": "^0.4.8",
        "@changesets/cli": "^2.26.1",
        "@types/node": "^18.15.9",
        "turbo": "^1.9.3",
        "typescript": "^4.9.5",
        "unbuild": "^1.1.2",
        "vitest": "^0.23.4"
    }
}
